{
  "mcpServers": {
    "google-sheets": {
      "command": "uvx",
      "args": ["mcp-google-sheets@latest"],
      "env": {
        "CREDENTIALS_PATH": "${GOOGLE_OAUTH_CREDENTIALS:-${CLAUDE_PLUGIN_ROOT}/credentials.json}",
        "TOKEN_PATH": "${HOME}/.claude/google-oauth/token.json"
      }
    }
  }
}
